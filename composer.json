{
    "name": "kapersoft/cron-job-api-for-laravel",
    "description": "Laravel package for Cron Job API",
    "license": "MIT",
    "type": "library",
    "version": "0.0.1",
    "keywords": [
        "laravel",
        "cron",
        "job",
        "api"
    ],
    "authors": [
        {
            "name": "Jan Willem Kaper",
            "email": "kapersoft@gmail.com"
        }
    ],
    "homepage": "https://github.com/kapersoft/cron-job-api-for-laravel",
    "require": {
        "kapersoft/cron-job-api": "^0.0.1",
        "laravel/framework": "^12.0"
    },
    "require-dev": {
        "driftingly/rector-laravel": "^2.0",
        "ergebnis/composer-normalize": "^2.45",
        "larastan/larastan": "^3.2",
        "laravel/facade-documenter": "dev-main",
        "laravel/pint": "^1.14",
        "orchestra/testbench": "^10.1",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "pestphp/pest-plugin-type-coverage": "^4.0",
        "phpstan/extension-installer": "^1.3 || ^2.0",
        "phpstan/phpstan-deprecation-rules": "^1.1 || ^2.0",
        "phpstan/phpstan-phpunit": "^1.3 || ^2.0",
        "rector/rector": "^2.0",
        "spatie/invade": "^2.1",
        "spatie/laravel-ray": "^1.35"
    },
    "repositories": {
        "facade-documenter": {
            "type": "vcs",
            "url": "git@github.com:laravel/facade-documenter.git"
        }
    },
    "autoload": {
        "psr-4": {
            "Kapersoft\\CronJobApiForLaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kapersoft\\CronJobApiForLaravel\\CronJobApiServiceProvider"
            ]
        }
    },
    "scripts": {
        "format": [
            "composer normalize",
            "@php ./vendor/bin/rector process",
            "@php ./vendor/bin/pint --repair"
        ],
        "lint": [
            "composer normalize --dry-run",
            "@php ./vendor/bin/rector process --dry-run",
            "@php vendor/bin/pint --ansi",
            "@php vendor/bin/phpstan analyse --verbose --ansi"
        ],
        "test": [
            "@php vendor/bin/pest"
        ]
    }
}
